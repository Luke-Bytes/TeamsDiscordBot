{
  "name": "discord-teams-bot",
  "private": true,
  "version": "1.0.0",
  "description": "Discord teams bot",
  "main": "src/index.ts",
  "type": "module",
  "engines": {
    "node": ">=18.x"
  },
  "scripts": {
    "build": "tsc && npm run postbuild",
    "postbuild": "node replace-imports.js",
    "start:bot:dev": "node start-bot-dev.js",
    "start:bot:prod": "node --experimental-specifier-resolution=node dist/BotManager.js",
    "dev": "npm run start:bot:dev",
    "prod": "npm run build && npm run start:bot:prod"
  },
  "author": "Luke",
  "license": "GNU",
  "dependencies": {
    "@types/node": "^22.5.4",
    "discord.js": "^14.16.1",
    "dotenv": "^16.0.3"
  },
  "devDependencies": {
    "concurrently": "^9.0.0",
    "cross-env": "^7.0.3",
    "nodemon": "^3.1.4",
    "replace-in-file": "^6.3.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.2"
  }
}
