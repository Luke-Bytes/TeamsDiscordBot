{
  "name": "discord-teams-bot",
  "private": true,
  "version": "1.0.0",
  "description": "Discord teams bot",
  "engines": {
    "node": ">=18.x"
  },
  "scripts": {
    "build": "npx prisma generate --no-hints && tsc",
    "start:bot:prod": "node dist/main.js",
    "dev": "tsx watch src/main.ts",
    "prod": "npm run build && npm run start:bot:prod",
    "dev:pm2": "pm2 start src/main.ts --name 'TeamsBot' --watch --interpreter tsx",
    "prod:pm2": "pm2 start dist/main.js --name 'TeamsBot' --max-restarts 5",
    "stop:pm2": "pm2 stop TeamsBot",
    "clean": "rimraf dist",
    "lint": "eslint .",
    "format": "prettier --write .",
    "lint:fix": "eslint . --fix",
    "check-format": "prettier --check ."
  },
  "author": "Luke and Eric",
  "license": "GNU",
  "dependencies": {
    "@eslint/js": "^9.12.0",
    "@prisma/client": "^5.20.0",
    "@types/ws": "^8.5.12",
    "chrono-node": "^2.7.7",
    "discord.js": "^14.16.3",
    "dotenv": "^16.0.3",
    "prisma": "^5.20.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.9.0",
    "concurrently": "^9.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.7",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3"
  }
}
